<div class="container">
<h5>タグが<%=@tag.name%>の投稿一覧</h5>

<% @events.each do |event| %>

  <div class="card">
    <div class="card-body">
      <h6 class="card-title">
        <%=link_to event_path(event.id) do %>
          <%= event.title %>
        <% end %>
      </h6>
    </div>
  </div>
<% end %>

